    var ufFilter = []

    inputUf.addEventListener('change', () => {
        let selectedUfIndex = document.querySelector('.uf-input').options.selectedIndex
        let selectedUf = document.querySelector('.uf-input').options[selectedUfIndex].value

            ufFilter = trocar(selectedUf)

            var strCities = ufFilter.map(item => {
              return (item.Cidade)
              })

             var uniqueCities = strCities.filter((item, index) => {
              return strCities.indexOf(item) === index
              })

             uniqueCities = uniqueCities.sort()

        var customCityValues = '<option value="" disabled selected>SELECIONE A CIDADE</option>'

        for (var i = 0; i < uniqueCities.length; i++){
        customCityValues += ` <option value="${uniqueCities[i]}">${uniqueCities[i]}</option> `
        }

        document.querySelector('.city-input').innerHTML = customCityValues

        return ufFilter
    })

    var btnSearch = document.querySelector('.btn-search')

    btnSearch.addEventListener('click', (e) => {
        e.preventDefault()
        updateMap()
    })

    function dados(item){
