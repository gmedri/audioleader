    }

    function trocar(uf){
      var filter = cities.filter((city) => {
      return city.Uf == uf
      } )
      return filter
    }

    function updateMap() {
        let selectedUfIndex = document.querySelector('.uf-input').options.selectedIndex
        let selectedUf = document.querySelector('.uf-input').options[selectedUfIndex].value

        let selectedCityIndex = document.querySelector('.city-input').options.selectedIndex
        let selectedCity = document.querySelector('.city-input').options[selectedCityIndex].value

        let targetMap = `https://maps.google.com/maps?q=${selectedCity},+-+${selectedUf}&t=m&z=10&output=embed&iwloc=near`
        document.querySelector('.elementor-custom-embed iframe').src = targetMap

    	dados(ufFilter)
    }
}



